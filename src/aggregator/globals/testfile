MONGO_PORT=27017
MONGO_USER_NAME=logger
MONGO_USER_PASSWORD=qwer1234
MONGO_COLLECTION=hcctv_echo_logs

## MONGO SPECIAL VAR
MONGO_INITDB_DATABASE=logs
MONGO_INITDB_ROOT_USERNAME=admin
MONGO_INITDB_ROOT_PASSWORD=qwer1234

# # NGINX
# NGINX_SERVICE_NAME=proxy-dev
# NGINX_USER=NGINX
# NGINX_WORKER_PROCESSES=1
# NGINX_HOST=localhost
# NGINX_HTTP_PORT=80
# NGINX_HTTPS_PORT=443

# NGINX
#NGINX_USER=nginx
# Worker
NGINX_WORKER_PROCESSES=1
NGINX_WORKER_CONNECTION=1024
NGINX_HOST=localhost
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

#Other options
LOG_DIR=logs
#### HCCTV environment

# GENERAL
PROJECT_NAME=HCCTVServer
TZ=Asia/Seoul

# SERVER
ENV=dev
ECHO_ENV=development
DOMAIN_PORT=4000
URI=http://localhost
HOST=localhost
ECHO_DOCKER_NAME=echo-dev
AWS=172.31.42.162
AWS_PORT=80

# MYSQL (MAIN)
MYSQL_FRAMEWORK=mysql
MYSQL_HOST=dev_db
MYSQL_PORT=3306
MYSQL_USER=test
MYSQL_PASSWORD=qwer1234
MYSQL_ROOT_PASSWORD=qwer1234
MYSQL_DATABASE=test
MYSQL_MULTIPLE_STATEMENT=true
MYSQL_POOL_WAITFORCONNECTION=10
MYSQL_POOL_CONNETIONLIMIT=10
MYSQL_POOL_QUEUELIMIT=0

